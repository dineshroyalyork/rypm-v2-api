model income_source {
  id            String   @id @default(uuid())
  tenant_id        String
  first_name     String
  last_name      String
  email         String
  mobile_number  String
  relationship  String

  created_at     DateTime @default(now())
  updated_at     DateTime @updatedAt

  tenant        tenants            @relation(fields: [tenant_id], references: [id])
}